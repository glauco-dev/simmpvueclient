{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.json", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "allowImportingTsExtensions":true,
    "verbatimModuleSyntax": true,
    "composite": true,
    "noEmit": true,
    "lib": ["ES2021"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  
}
